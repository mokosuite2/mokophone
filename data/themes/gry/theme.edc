                }
                rel2 {
                    relative: 0.0  1.0;
                    offset:   4   -5;
                }
            }
        }
        part { name: "elm.swallow.icon";
            clip_to: "disclip";
            type: SWALLOW;
            description {
                state: "default" 0.0;
