        part { name: "elm.swallow.end";
            clip_to: "disclip";
            type: SWALLOW;
            mouse_events: 0;
            description {
                state: "default" 0.0;
                fixed: 1 0;
                align: 1.0 0.5;
                aspect: 1.0 1.0;
                aspect_preference: VERTICAL;
                rel1 {
                    relative: 1.0  0.0;
                    offset:   -5    10;
                }
                rel2 {
                    relative: 1.0  1.0;
                    offset:   -10   -5;
                }
            }
        }
        part { name: "elm.text";
            clip_to: "disclip";
            type:           TEXT;
            mouse_events:   0;
            scale: 1;
            description {
                state: "default" 0.0;
                rel1 {
                    to_x:     "elm.swallow.icon";
                    relative: 1.0  0.0;
                    offset:   4 4;
                }
                rel2 {
                    to_x:     "elm.swallow.end";
                    relative: 0.0  0.5;
                    offset:   -1 0;
                }
                color: 0 0 0 255;
                color3: 0 0 0 0;
                text {
                    font: "redensek";
                    size: 10;
                    elipsis: 0.0;
                    min: 0 1;
                    align: 0.0 1.0;
                }
            }
        }
        part { name: "elm.text.sub";
            clip_to: "disclip";
            type:           TEXT;
            mouse_events:   0;
            scale: 1;
            description {
                state: "default" 0.0;
                //               min: 16 16;
               rel1 {
                  to_x:     "elm.swallow.icon";
                  relative: 1.0  0.5;
                  offset:   4 4;
               }
               rel2 {
                  to_x:     "elm.swallow.end";
                  relative: 0.0  1.0;
                  offset:   -1 -5;
               }
                color: COLOR_DARK_GRAY;
                color3: 0 0 0 0;
                text {
                    font: "redensek";
                    size: 9;
                    elipsis: 0.0;
                    min: 0 1;
                    align: 0.0 0.0;
                }
            }
        }
        part { name: "disclip";
            type: RECT;
            description {
                state: "default" 0.0;
                rel1.to: "bg";
                rel2.to: "bg";
            }
            description {
                state: "disabled" 0.0;
                inherit: "default" 0.0;
                color: 255 255 255 64;
            }
        }
    }

    programs {
        // signal: elm,state,%s,active
        //   a "check" item named %s went active
        // signal: elm,state,%s,passive
        //   a "check" item named %s went passive
        // default is passive

        program {
            name:    "go_active";
            signal:  "elm,state,selected";
            source:  "elm";
            action:  STATE_SET "selected" 0.0;
            target:  "bg";
            target:  "elm.text";
            target:  "elm.text.sub";
        }
        program {
            name:    "go_passive";
            signal:  "elm,state,unselected";
            source:  "elm";
            action:  STATE_SET "default" 0.0;
            target:  "bg";
            target:  "elm.text";
            target:  "elm.text.sub";
            transition: LINEAR 0.1;
        }
        program {
            name:    "go_disabled";
            signal:  "elm,state,disabled";
            source:  "elm";
            action:  STATE_SET "disabled" 0.0;
            target:  "disclip";
        }
        program {
            name:    "go_enabled";
            signal:  "elm,state,enabled";
            source:  "elm";
            action:  STATE_SET "default" 0.0;
            target:  "disclip";
        }
    }
