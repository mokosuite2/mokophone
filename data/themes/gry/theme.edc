                action: STATE_SET "default" 0.0;
                target: "button_rhomb";
            }
        } /* Close Prog */
    }   /* keypad */


    group { name: "slider";

        images {
            image: "slider.png" COMP;
            image: "sliderbackground_left.png" COMP;
            image: "sliderbackground_middle.png" COMP;
            image: "sliderbackground_right.png" COMP;
        }

        parts {

            part {
                name: "slider-bg";
                type: RECT;
                mouse_events: 0;
                description {
                    state: "default" 0.0;
                    color: 0 0 0 0;
                    min: 225*SCALE_FACTOR 36*SCALE_FACTOR;
                    rel1 { relative: 0 0; }
                    rel2 { relative: 1.0 1.0; }
                }
            }

            part {
                name: "clipper";
                type: "RECT";
                mouse_events: 0;
                description {
                    state: "default" 0.0;
                    rel1.to: "slider-bg";
                    rel2.to: "slider-bg";
                    color: 255 255 255 255;
                    }
            }

            part {
                name: "slider";
                type: RECT;
                mouse_events: 1;
                repeat_events: 1;
                description {
                    state: "default" 0.0;
                    fixed: 1 1;
                    min: 60*SCALE_FACTOR 36*SCALE_FACTOR;
                    color: 0 0 0 0;
                    rel1 { to: "slider-bg"; relative: 0.0 0.0; }
                    rel2 { to: "slider-bg"; relative: 1.0 1.0; }
                }
                description {
                    state: "hidden" 0.0;
                    inherit: "default" 0.0;
                    visible: 0;
                }
                dragable {
                    confine: "slider-bg";
                    events: "slider";
                    x: 1 1 0;
                    y: 0 0 0;
                }
            }

            part {
                name: "sliderbackground-middle";
                type: "IMAGE";
                mouse_events: 0;
                description {
                    state: "default" 0.0;
                    image.normal: "sliderbackground_middle.png";
                    rel1 { to: "slider-bg"; relative: 0.0 0.0; offset: 12*SCALE_FACTOR 0; }
                    rel2 { to: "slider-bg"; relative: 1.0 1.0; offset: -12*SCALE_FACTOR 0; }
                    }
            }

            part {
                name: "sliderbackground-left";
                type: "IMAGE";
                mouse_events: 0;
                description {
                    state: "default" 0.0;
                    image.normal: "sliderbackground_left.png";
                    rel1.to: "slider-bg";
                    rel2.to: "slider-bg";
                    rel2.relative: 0.0 1.0;
                    rel2.offset: 12*SCALE_FACTOR 0;
                }
            }

            part {
                name: "sliderbackground-right";
                type: "IMAGE";
                mouse_events: 0;
                description {
                    state: "default" 0.0;
                    image.normal: "sliderbackground_right.png";
                    rel1.to: "slider-bg";
                    rel1.relative: 1.0 0.0;
                    rel1.offset: -12*SCALE_FACTOR 0;
                    rel2.to: "slider-bg";
                    }
            }

            part {
                name: "slider-text";
                type: TEXT;
                mouse_events: 0;
                clip_to: "clipper";
                description {
                    state: "default" 0.0;
                    color: 255 255 255 255;
                    //fixed: 1 1;
                    rel1.to: "slider-bg";
                    rel2.to: "slider-bg";
                    text {
                        text: "accept";
                        font: "redensek:style=bold";
                        size: 18*SCALE_FACTOR;
                        min: 0 1;
                        align: 0.5 0.5;
                    }
                }
            }

            part {
                name: "arrow";
                type: "IMAGE";
                mouse_events: 0;
                clip_to: "clipper";
                description {
                    state: "default" 0.0;
                    image.normal: "slider.png";
                    rel1.to: "slider";
                    rel2.to: "slider";
                }
                description {
                    state: "hidden" 0.0;
                    inherit: "default" 0.0;
                    visible: 0;
                }
            }


        }   /* end parts */

        programs {

            program {
                name: "drag_begin";
                signal: "mouse,down,1";
                source: "slider";
                action: SIGNAL_EMIT "beginDrag" "slider";
            }

            program {
                name: "drag_end";
                signal: "mouse,up,1";
                source: "slider";
                action: SIGNAL_EMIT "finishDrag" "slider";
            }

            program {
                name: "slider_end";
                signal: "mouse,up,1";
                source: "slider";
                script {
                    new Float:dx, Float:dy;
                    get_drag(PART:"slider", dx, dy);
                    if (dx >= 0.9) {
                        set_state(PART:"slider", "hidden", 0.0);
                        set_state(PART:"arrow", "hidden", 0.0);

                        //set_drag(PART:"slider", 0.0, 0.0);
                        emit("unlockScreen", "slider");
                        }
                    else {
                        set_drag(PART:"slider", 0.0, 0.0);
                        }
                }
            }

        }   /* end programs */

    }


} /* collections */
